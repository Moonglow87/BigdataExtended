<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <HBox alignment="CENTER" prefHeight="42.0" prefWidth="639.0" spacing="10.0">
            <children>
                <TextField id="queryField" fx:id="queryField" />
                <ComboBox fx:id="comboBox" prefWidth="150.0" />
                <Button fx:id="button" alignment="CENTER" mnemonicParsing="false" onAction="#OnButtonClick" text="Search" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <TabPane fx:id="tabPane" prefHeight="357.0" prefWidth="624.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="tableTab" onSelectionChanged="#TableTabActive" text="Table" />
                <Tab fx:id="chartTab" onSelectionChanged="#ChartTabActive" text="Chart" />
                <Tab fx:id="mapTab" onSelectionChanged="#MapTabActive" text="Map">
               <content>
                  <ScrollPane fx:id="mapPane" prefHeight="200.0" prefWidth="200.0">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                    </content>
                  </ScrollPane>
               </content></Tab>
                <Tab fx:id="rChartTab" onSelectionChanged="#RChartTabActive" text="R-Chart" />
                <Tab fx:id="tableTab2" onSelectionChanged="#TableTabActive" text="Table2">
              <content>
                  <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <AnchorPane>
                           <children>
                             <!--<AnchorPane minHeight="-Infinity" minWidth="-Infinity">-->
                                 <!--<children>-->
                                    <TableView fx:id="table" prefHeight="311.0" prefWidth="631.0" VBox.vgrow="ALWAYS">
                                 <VBox.margin>
                                    <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                                 </VBox.margin>
                              </TableView>
                           </children>
                        </AnchorPane>
                     </children>
                  </VBox>
                     <!--</children>-->
                     <!--<padding>-->
                        <!--<Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />-->
                     <!--</padding>-->
                  <!--</AnchorPane>-->
              </content>
            </Tab>
            </tabs>
        </TabPane>
    </children>
</VBox>
